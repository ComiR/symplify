#!/usr/bin/env php
<?php

declare(strict_types=1);

use Symplify\Gerudoc\Bootstrap\ComposerAutoloader;
use Symplify\Gerudoc\HttpKernel\GerudocBuilderKernel;
use Symplify\SymplifyKernel\Bootstrap\KernelBootAndApplicationRun;

require_once __DIR__ . '/../src/Bootstrap/ComposerAutoloader.php';

$composerAutoloader = new ComposerAutoloader();
$composerAutoloader->load();

$kernelBootAndApplicationRun = new KernelBootAndApplicationRun(GerudocBuilderKernel::class);
$kernelBootAndApplicationRun->run();
